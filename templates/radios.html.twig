{#
/**
 * @file
 * Theme override for a 'radios' #type form element.
 *
 * Available variables
 * - attributes: A list of HTML attributes for the wrapper element.
 * - children: The rendered radios.
 *
 * @see template_preprocess_radios()
 */
#}
{% set wrapperclass = "radio" %}
{% if custom_inline %}
    {% set wrapperclass = wrapperclass ~ " custom-control-wrapper-inline" %}
{% endif %}
<div{{ attributes.addClass(wrapperclass) }}>{{ children }}</div>
